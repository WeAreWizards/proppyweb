{
  "name": "proppyweb",
  "version": "0.0.0",
  "description": "A collaborative proposal website",
  "scripts": {
    "clean": "rm -rf dist && rm -rf build",
    "build": "rm -rf build && webpack",
    "dist": "export NODE_ENV=production; rm -rf dist && webpack --config=webpack.prod.config.js",
    "start": "webpack && webpack-dev-server",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WeAreWizards/proppyweb.git"
  },
  "author": "We Are Wizards",
  "bugs": {
    "url": "https://github.com/WeAreWizards/proppyweb/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "homepage": "https://github.com/WeAreWizards/proppyweb#readme",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/classnames": "^2.2.0",
    "@types/jest": "^21.1.0",
    "@types/lodash": "^4.14.71",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^15.5.1",
    "@types/react-joyride": "^1.10.1",
    "@types/uuid": "^3.4.0",
    "autoprefixer": "^7.1.2",
    "chai": "^4.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.0.0",
    "lost": "^8.2.0",
    "mobx-react-devtools": "^4.2.15",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0",
    "react-transform-hmr": "^1.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.0.0",
    "ts-loader": "^3.0.0",
    "tslint": "^5.5.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "classnames": "^2.1.5",
    "es6-promise": "^4.1.1",
    "html-janitor": "^2.0.2",
    "lodash": "^4.15.0",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "raven-js": "^3.17.0",
    "react": "^15.6.1",
    "react-autosize-textarea": "^0.4.8",
    "react-dom": "^15.6.1",
    "react-joyride": "^1.10.1",
    "router5": "^5.1.3",
    "signature_pad": "^2.2.1",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.3",
    "zenscroll": "^4.0.0"
  }
}
